@model IEnumerable<CuaHangThietBiDienTu.Context.PHIEUNHAP>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/ADMIN/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h2 style="text-align:center">Danh sách phiếu nhập</h2>
    <a href="@Url.Action("Create", "PHIEUNHAPs")" class="btn btn-primary m-2">Tạo mới</a>
    <table class="table">
        <tr>
            <th>
                Ngày nhập
            </th>
            <th>
                Tên nhân viên
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NGAYNHAP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NHANVIEN.TENNV)
                </td>
                
                <td>
                    <a href="@Url.Action("Edit", "PHIEUNHAPs", new { id = item.MAPN })">
                        <img src="~/Images/edit.png" alt="Edit" class="icon" width="40px">
                    </a>
                    <a href="@Url.Action("Details", "PHIEUNHAPs", new { id = item.MAPN })">
                        <img src="~/Images/details.png" alt="Edit" class="icon" width="40px">
                    </a>
                    <a href="@Url.Action("Delete", "PHIEUNHAPs", new { id = item.MAPN })">
                        <img src="~/Images/delete.png" alt="Edit" class="icon" width="40px">
                    </a>
                </td>
            </tr>
        }

    </table>
</div>



