@model List<CuaHangThietBiDienTu.Context.LOAISANPHAM>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="section-content padding-y">
    <div class="container">

        <nav class="row">
            @foreach (var item in Model)
            {
                <div class="col-md-3">
                    <div class="card card-category">
                        <div class="img-wrap" style="background: #ffd7d7">

                        </div>
                        <div class="card-body">
                            <h4 class="card-title"><a href="#">@item.TENLOAISP</a></h4>
                         
                        </div>
                    </div>

                </div>
            }

        </nav> <!-- row.// -->


    </div> <!-- container .//  -->
</section>
<!-- ========================= SECTION CONTENT END// ========================= -->
